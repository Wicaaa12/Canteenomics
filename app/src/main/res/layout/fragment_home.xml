<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/main_bg"
    tools:context=".ui.home.HomeFragment">

    <Button
        android:id="@+id/canteen1_button"
        android:layout_width="400dp"
        android:layout_height="80dp"
        android:background="@drawable/round_button"
        android:drawableLeft="@drawable/sisiig"
        android:fontFamily="casual"
        android:insetTop="30dp"
        android:text="Mang Boy"
        android:textColor="#000000"
        android:textColorHighlight="@color/black"
        android:textColorLink="@color/gray"
        android:textSize="16sp"
        app:backgroundTint="@color/gray"
        app:layout_constraintBottom_toTopOf="@+id/canteen2_button"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.466"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.962" />

    <Button
        android:id="@+id/canteen3_button"
        android:layout_width="400dp"
        android:layout_height="80dp"
        android:background="@drawable/round_button"
        android:drawableLeft="@drawable/sisiig"
        android:fontFamily="casual"
        android:insetTop="30dp"
        android:text="Ate Auring"
        android:textColor="#000000"
        android:textColorHighlight="@color/black"
        android:textColorLink="@color/gray"
        android:textSize="16sp"
        app:backgroundTint="@color/gray"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.466"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.602" />

    <Button
        android:id="@+id/canteen4_button"
        android:layout_width="400dp"
        android:layout_height="80dp"
        android:layout_marginBottom="16dp"
        android:background="@drawable/round_button"
        android:drawableLeft="@drawable/sisiig"
        android:fontFamily="casual"
        android:insetTop="30dp"
        android:text="Nanay ni Isaac"
        android:textColor="#000000"
        android:textColorHighlight="@color/black"
        android:textColorLink="@color/gray"
        android:textSize="16sp"
        app:backgroundTint="@color/gray"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.466"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/canteen3_button"
        app:layout_constraintVertical_bias="0.054" />

    <Button
        android:id="@+id/canteen2_button"
        android:layout_width="400dp"
        android:layout_height="80dp"
        android:layout_marginBottom="8dp"
        android:background="@drawable/round_button"
        android:drawableLeft="@drawable/sisiig"
        android:fontFamily="casual"
        android:insetTop="30dp"
        android:text="Lala Lou"
        android:textColor="#000000"
        android:textColorHighlight="@color/black"
        android:textColorLink="@color/gray"
        android:textSize="16sp"
        app:backgroundTint="@color/gray"
        app:layout_constraintBottom_toTopOf="@+id/canteen3_button"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.466"
        app:layout_constraintStart_toStartOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>